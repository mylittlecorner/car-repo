@model IEnumerable<NETTEST2.Models.UserPage>
@if (Model.ToList() != null)
{
    foreach (var elements in Model.ToList())
    {
        foreach (var item in elements.Offers)
        {
            ViewBag.item = item;
            ViewBag.UserAuth = elements.UserAuth;
            @Html.Partial("_OfferboxPartial")
        }
    }
}